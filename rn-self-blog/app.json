{
  "expo": {
    "name": "Self Blog",
    "slug": "rn-self-posts",
    "description": "React Native application for self blog",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.szharkov88.self-blog-rn-app",
      "buildNumber": "1.0.0",
      "splash": {
        "image": "./assets/rn-splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#242832"
      }
    },
    "android": {
      "package": "com.szharkov88.self_blog_rn_app",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"],
      "splash": {
        "image": "./assets/rn-splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#242832"
      }
    }
  }
}
